
// The initialization code follows the Smart Script code below

<script src="https://onelinksmartscript.appsflyer.com/onelink-smart-script-latest.js"></script>

//OneLink引数
var oneLinkURL = "https://famipay.onelink.me/sVp0";
var mediaSource = {keys:["utm_source"],defaultValue:"LP_fromQR"};
var campaign = {keys:["utm_campaign"],defaultValue:"famipayroan_202310"};
var is_retargeting = {paramKey:"is_retargeting",defaultValue:"true"};
var channel = {keys:["utm_channel"],defaultValue:"any_video"};
var custom_ss_ui = {paramKey:"af_ss_ui",defaultValue:"true"};

//window.AF_SMART_SCRIPT.定義されたパラメータマッチングリストを基にOneLinkを生成
var result = window.AF_SMART_SCRIPT.generateOneLinkURL({
    oneLinkURL: oneLinkURL,
    afParameters: {
        mediaSource: mediaSource,
        campaign: campaign,
        afCustom: [
            is_retargeting,
            custom_ss_ui
        ],
        channel: channel
    }
});
//スマートスクリプトの結果を確認
      var result_url = AF_SMART_SCRIPT_RESULT.clickURL;
      if (result_url) {
            document.getElementById('final_link').setAttribute('href', result_url);      
            window.AF_SMART_SCRIPT.displayQrCode("qr");
            window.AF_SMART_SCRIPT.fireImpressionsLink();   // [オプション]impression計測(ページロードの度に計測される) 
    }

// If needed, you can download the script from: https://onelinksmartscript.appsflyer.com/onelink-smart-script-latest.js




