<script>

//OneLink引数
var oneLinkURL = "https://onyasai.onelink.me/IKOB";
var mediaSource = {keys:["utm_source"],defaultValue:"officialsite"};
var campaign = {keys:["utm_campaign"],defaultValue:"campaign_officialsite"};
var googleClickIdKey = "af_sub1";
var custom_ss_ui = {paramKey:"af_ss_ui",defaultValue:"true"};
var custom_ss_gtm_ui = {paramKey:"af_ss_gtm_ui",defaultValue:"true"};

// 定義されたパラメータマッチングリストを基にOneLinkを生成
AF_SMART_SCRIPT_RESULT = window.AF_SMART_SCRIPT.generateOneLinkURL({
    oneLinkURL: oneLinkURL,
    afParameters: {
        mediaSource: mediaSource,
        campaign: campaign,
        googleClickIdKey: googleClickIdKey,
        afCustom: [
            custom_ss_ui,
            custom_ss_gtm_ui
        ]
    }
});
    // 生成されたOneLinkをLP内の特定のCTA URLタグ（aタグ）に置き換える指示
    if (AF_SMART_SCRIPT_RESULT && AF_SMART_SCRIPT_RESULT.clickURL) {
        document.querySelectorAll('a[id="final_url"]').forEach(function (e) {  // 生成された最終URLをLP内のaタグのどのリンクと置き換えるのか、指示してください。       
            e.setAttribute('href', AF_SMART_SCRIPT_RESULT.clickURL);
        });
        window.AF_SMART_SCRIPT.displayQrCode("qr");
    };
    
</script>
